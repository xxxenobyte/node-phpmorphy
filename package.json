  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antixrist/node-phpmorphy"
  },
  "homepage": "https://github.com/antixrist/node-phpmorphy",
  "author": {
    "name": "antixrist"
